{
    "name": "sajidifti/management-panel",
    "description": "A database-independent management panel for Laravel applications with emergency access capabilities",
    "keywords": [
        "laravel",
        "management",
        "panel",
        "admin",
        "emergency",
        "database-independent"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sajid Ifti",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Sajidifti\\ManagementPanel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sajidifti\\ManagementPanel\\ManagementPanelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
